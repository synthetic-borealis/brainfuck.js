{
  "name": "hirnfick",
  "author": "Elhanan Flesch",
  "license": "MIT",
  "version": "9.1.0",
  "description": "A Brainfuck source to source compiler",
  "homepage": "https://synthetic-borealis.github.io/hirnfick/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synthetic-borealis/hirnfick.git"
  },
  "bugs": {
    "url": "https://github.com/synthetic-borealis/hirnfick/issues"
  },
  "keywords": [
    "brainfuck",
    "bf",
    "esolang",
    "language",
    "compiler"
  ],
  "type": "module",
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "bin": "bin/hirnfick.js",
  "files": [
    "lib/",
    "types/",
    "dist/",
    "bin/",
    "docs/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src/**/*.{ts,js} __tests__/**/*.{ts,js} scripts/**/*.{ts,js}",
    "lint:bin": "eslint bin/**/*.{js,cjs,mjs}",
    "clean": "rimraf types/ lib/ dist/",
    "docs": "typedoc",
    "build:node": "tsc",
    "build:browser": "ts-node scripts/bundle.ts",
    "deno:clean": "rimraf deno/",
    "deno:build": "ts-node-esm scripts/deno.ts",
    "build": "npm run build:node && npm run build:browser",
    "prepack": "npm run clean && npm run build",
    "postpack": "npm run clean",
    "prepublish": "in-publish && npm run clean && npm run build || not-in-publish",
    "postpublish": "npm run clean",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/minimist": "^1.2.3",
    "@types/node": "^18.18.4",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cpp-utils": "^3.0.2",
    "esbuild": "^0.17.19",
    "eslint": "^8.51.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.2",
    "in-publish": "^2.0.1",
    "jest": "^29.7.0",
    "pascal-utils": "^2.0.2",
    "python-shell": "^3.0.1",
    "rimraf": "^4.4.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "minimist": "^1.2.8"
  },
  "packageManager": "yarn@3.2.4"
}
