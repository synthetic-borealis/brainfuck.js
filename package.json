{
  "name": "brainfuck",
  "version": "1.0.0",
  "description": "A Brainfuck to JavaScript transpiler\"",
  "files": [
    "index.js",
    "es",
    "lib",
    "README.md",
    "brainfuck.js",
    "brainfuck.min.js"
  ],
  "keywords": [
    "brainfuck"
  ],
  "scripts": {
    "lint": "eslint src",
    "lint:log": "eslint src > lint.log",
    "lint:fix": "eslint --fix src",
    "clean:node": " rimraf index.js lib",
    "clean:es": "rimraf es",
    "clean:browser": "rimraf brainfuck*.js",
    "clean": "run-p clean:*",
    "minify": "uglifyjs brainfuck.js -o brainfuck.min.js --compress --mangle --comments /Copyright/",
    "build:browser": "node --require @babel/register build-browser && npm run minify",
    "build:es": "babel src -d es --env-name=es",
    "build:node": "babel src -d .",
    "build": "run-p build:*",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "Elhanan Flesch",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@babel/register": "^7.16.8",
    "@rollup/plugin-babel": "^5.3.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.63.0",
    "uglify-js": "^3.14.5"
  }
}
