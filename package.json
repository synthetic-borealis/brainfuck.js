{
  "name": "hirnfick",
  "author": "Elhanan Flesch",
  "license": "MIT",
  "version": "8.0.1",
  "description": "A Brainfuck source to source compiler",
  "homepage": "https://synthetic-borealis.github.io/hirnfick/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synthetic-borealis/hirnfick.git"
  },
  "bugs": {
    "url": "https://github.com/synthetic-borealis/hirnfick/issues"
  },
  "keywords": [
    "brainfuck",
    "bf",
    "esolang",
    "language",
    "compiler"
  ],
  "main": "lib/index.js",
  "types": "types/index.d.ts",
  "bin": {
    "hirnfick": "bin/hirnfick.js"
  },
  "files": [
    "lib/",
    "types/",
    "dist/",
    "bin/",
    "docs/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint src/**/*.{ts,js} __tests__/**/*.{ts,js} scripts/**/*.{ts,js}",
    "lint:bin": "eslint bin/**/*.{js,cjs,mjs}",
    "clean": "rimraf types/ lib/ dist/",
    "docs": "typedoc",
    "build:node": "tsc",
    "build:browser": "ts-node scripts/bundle.ts",
    "deno:clean": "rimraf deno/",
    "deno:build": "ts-node scripts/deno.ts",
    "build": "run-p build:*",
    "prepack": "yarn clean && yarn build",
    "postpack": "yarn clean",
    "prepublish": "in-publish && yarn clean && yarn build || not-in-publish",
    "postpublish": "yarn clean",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.1.0",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.7.23",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "cpp-utils": "^3.0.0",
    "esbuild": "^0.15.9",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.0.4",
    "in-publish": "^2.0.1",
    "jest": "^29.1.1",
    "npm-run-all": "^4.1.5",
    "pascal-utils": "^2.0.0",
    "python-shell": "^3.0.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.15",
    "typedoc-plugin-markdown": "^3.13.6",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "minimist": "^1.2.6"
  }
}
