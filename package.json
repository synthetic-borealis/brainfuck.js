{
  "name": "hirnfick",
  "author": "Elhanan Flesch",
  "license": "MIT",
  "version": "5.0.0",
  "description": "A Brainfuck source to source compiler",
  "homepage": "https://github.com/synthetic-borealis/hirnfick.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/synthetic-borealis/hirnfick.js.git"
  },
  "bugs": {
    "url": "https://github.com/synthetic-borealis/hirnfick.js/issues"
  },
  "keywords": [
    "brainfuck",
    "language",
    "compiler"
  ],
  "main": "index.js",
  "bin": {
    "hirnfick": "bin/hirnfick.js"
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib/",
    "dist/",
    "bin/",
    "examples/",
    "docs/",
    "README.md"
  ],
  "scripts": {
    "lint": "eslint lib/** utils/** bin/**",
    "lint:log": "eslint lib/** utils/** bin/** > lint.log",
    "lint:fix": "eslint --fix lib/** utils/**",
    "clean:ts-def": "del-cli **/*.d.ts !node_modules/**/*.d.ts",
    "clean:browser": "rimraf dist",
    "clean:qb": "del-cli *.bas",
    "clean:pascal": "del-cli *.pas",
    "clean:bin": "del-cli *.o *.exe",
    "clean": "run-p clean:*",
    "docs": "jsdoc2md index.js lib/**/*.js > docs/API.md",
    "build:ts-def": "npx -p typescript tsc index.js --declaration --allowJs --emitDeclarationOnly --outDir ./",
    "build:browser": "node build-browser.js",
    "build": "run-p build:*",
    "prepack": "yarn clean && yarn build",
    "postpack": "yarn clean",
    "prepublish": "in-publish && yarn clean && yarn build || not-in-publish",
    "postpublish": "yarn clean && yarn build",
    "codegen": "node utils/codegen.js",
    "test:qb": "node utils/qb-test.js",
    "test:pascal": "node utils/pascal-test.js",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "cpp-utils": "^1.0.1",
    "del-cli": "^4.0.1",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.8.3",
    "in-publish": "^2.0.1",
    "jest": "^27.5.1",
    "jsdoc-to-markdown": "^7.1.1",
    "npm-run-all": "^4.1.5",
    "python-shell": "^3.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "minimist": "^1.2.6"
  }
}
